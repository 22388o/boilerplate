import "shift.scrypt";

contract Test {
  public function unlock(int x, int n, int op, int expected) {
    
    int result = 0;
    if (pack(op) == "pow2") {
        result = Shift.pow2(n);
    }
    else if (pack(op) == "left") {
        result = Shift.left(x, n);
    }
    else if (pack(op) == "right") {
        result = Shift.right(x, n);
    }

    require(result == expected);
  }
}
